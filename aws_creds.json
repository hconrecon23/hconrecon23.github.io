{
    "rules": [
        {
            "name": "aws-access-token",
            "rule": "(?:A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}",
            "target": "all",
            "type": "regexp",
            "onlymatched": false,
            "action": [
                "stdout"
            ]
        }
    ],
    "groupname": "credentials",
    "active": true
}